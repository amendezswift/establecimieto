<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="fel_stock_warehouse_form" model="ir.ui.view">
    <field name="name">fel.stock.warehouse.form</field>
    <field name="model">stock.warehouse</field>
    <field name="inherit_id" ref="stock.view_warehouse" />
    <field name="arch" type="xml">
      <xpath expr="//field[@name='code']" position="after">
        <field name="establecimiento_id" options="{'no_create': True, 'no_create_edit': True}"
          domain="[('empresa_id', '=', company_id)]" />
      </xpath>
    </field>
  </record>
</odoo>